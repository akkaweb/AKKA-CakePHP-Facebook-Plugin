{
    "name": "akkaweb/cakephp-facebook",
    "description": "CakePHP 3 Facebook Plugin - Facebook SDKv5",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "cakephp3", "facebook"],
    "license": "MIT",
    "authors": [
	{
	    "name": "Andre Santiago",
	    "email": "admin@akkaweb.com"
	}
    ],
    "require": {
	"php": ">=5.4",
	"facebook/graph-sdk": "^5.0"
    },
    "require-dev": {
	"phpunit/phpunit": "*"
    },
    "autoload": {
	"psr-4": {
	    "AkkaFacebook\\": "src"
	}
    },
    "autoload-dev": {
	"psr-4": {
	    "AkkaFacebook\\Test\\": "tests",
	    "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
	}
    },
    "extra": {
	"installer-name": "AkkaFacebook"
    }
}
